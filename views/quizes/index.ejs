<form method='get' action='/quizes'>
  <input type='text' name='search' placeholder='Ingrese el término de búsqueda' required />
  <input type='submit' value='Buscar'/>
</form>
<ul>
<% var i; for (i = 0;i < quizes.length; i++) { %>
  <li><a href="quizes/<%= quizes[i].id %>"><%= quizes[i].pregunta %></a></li>
<% } %>
</ul>
